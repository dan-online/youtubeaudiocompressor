    "@sapphire/eslint-config": "^5.0.5",
    "@types/chrome": "^0.0.268",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "esbuild": "^0.21.5",
    "eslint": "^9.5.0",
    "prettier": "^3.3.2",
    "rollup": "^4.18.0",
    "rollup-plugin-chrome-extension": "^3.6.12",
    "rollup-plugin-empty-dir": "^1.0.5",
    "tslib": "^2.6.3",
