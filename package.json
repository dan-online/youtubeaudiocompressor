		"@rollup/plugin-typescript": "^12.3.0",
		"@sapphire/eslint-config": "^5.0.6",
		"@types/chrome": "^0.1.36",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"esbuild": "^0.27.3",
		"eslint": "^9.39.2",
		"prettier": "^3.8.1",
		"rollup": "^4.57.1",
		"rollup-plugin-chrome-extension": "^3.6.15",
		"rollup-plugin-empty-dir": "^1.0.5",
