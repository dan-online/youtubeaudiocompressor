    "@rollup/plugin-typescript": "^12.1.4",
    "@sapphire/eslint-config": "^5.0.6",
    "@types/chrome": "^0.1.12",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "rollup": "^4.52.0",
    "rollup-plugin-chrome-extension": "^3.6.15",
    "rollup-plugin-empty-dir": "^1.0.5",
